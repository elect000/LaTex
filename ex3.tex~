\begin{align}
 \left(
 \\begin{array}{cc}
  2&-1 \\
  -3 &4 \\
  \end{array}
 \right)
 \left(
 \begin{array}{c}
  1\\
  2\\
 \end{array}
 \right) = \left(
 \begin{array}{c}
  0\\
  5\\
 \end{array}
 \right)
\end{align}

